@page "/monkey"

<h1 style="text-align:center; margin-bottom:20px;">🐒 Monkey Explorer</h1>

@if (monkeys == null || !monkeys.Any())
{
    <h1>No Monkeys Found!</h1>
}
else
{
    <div class="monkey-grid">
        @foreach (var monkey in monkeys)
        {
            <div class="monkey-card">
                <img src="@monkey.Image" alt="@monkey.Name" class="monkey-img" />

                <h3>@monkey.Name</h3>
                <p><b>Location:</b> @monkey.Location</p>
                <p style="font-size:14px; opacity:0.8;">@monkey.Details</p>
            </div>
        }
    </div>
}

<style>
    .monkey-grid {
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
        gap: 25px;
        padding: 15px;
    }

    .monkey-card {
        background: white;
        border-radius: 12px;
        padding: 15px;
        box-shadow: 0 2px 8px rgba(0,0,0,0.15);
        transition: 0.25s ease-in-out;
        cursor: pointer;
    }

    .monkey-card:hover {
        transform: translateY(-6px);
        box-shadow: 0 6px 18px rgba(0,0,0,0.25);
    }

    .monkey-img {
        width: 100%;
        height: 170px;
        object-fit: cover;
        border-radius: 10px;
        margin-bottom: 10px;
    }
</style>

